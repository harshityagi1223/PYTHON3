   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def swap(nums : List[int], pos1 : int, pos2 : int):\n",
    "        nums[pos1],nums[pos2] =nums[pos2],nums[pos1]\n",
    "        print(nums)\n",
    "Solution.swap([1,2,3,4,5,6,7],3,5)"
   ]
